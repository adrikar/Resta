<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rest">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Rest">
        <activity android:name=".Admin.MenuAdmin"></activity><activity
        android:name=".Admin.MainActivityAdmin"
        android:label="@string/title_activity_main_admin"
        android:theme="@style/Theme.Rest.NoActionBar" />
        <activity
            android:name=".Admin.MainAdminActivity"
            android:label="@string/title_activity_main_admin"
            android:theme="@style/Theme.Rest.NoActionBar" />
        <activity
            android:name=".Admin.MainAdmin"
            android:label="@string/title_activity_main_admin"
            android:theme="@style/Theme.Rest.NoActionBar" />
        <activity android:name=".Admin.LoginAdmin" />

        <activity
            android:name=".MenuActivity"
            android:label="@string/title_activity_menu"
            android:theme="@style/Theme.Rest.NoActionBar" />
        <activity android:name=".Register" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>